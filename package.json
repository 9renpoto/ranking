  "version": "0.0.1",
  "author": "9renpoto",
  "dependencies": {
    "@firebase/app-types": "0.4.1",
    "@fortawesome/fontawesome-free": "5.9.0",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.18",
    "firebase": "6.3.1",
    "graphql-request": "1.8.2",
    "next": "9.0.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "5.0.0",
    "@9renpoto/eslint-config-react": "5.0.0",
    "@9renpoto/stylelint-config": "5.0.0",
    "@babel/core": "7.4.5",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-storyshots": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/cli": "5.1.9",
    "@storybook/react": "5.1.9",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.15",
    "@types/react-test-renderer": "16.8.2",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__addon-knobs": "5.0.3",
    "@types/storybook__addon-storyshots": "4.0.2",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/parser": "1.13.0",
    "@zeit/next-typescript": "1.1.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "codecov": "3.5.0",
    "concurrently": "4.1.1",
    "cross-env": "5.2.0",
    "eslint": "6.1.0",
    "eslint-plugin-jest": "22.13.6",
    "faker": "4.1.0",
    "husky": "3.0.1",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.3",
    "lint-staged": "9.2.0",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3",
    "typescript-styled-plugin": "0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "jest-styled-components"
    ],
    "setupFiles": [
      "./.storybook/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.tsx": [
      "stylelint",
      "prettier-eslint --write",
      "git add"
    ],
    "*.{md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/g59/rate.git",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "tsc -m commonjs",
    "build:storybook": "build-storybook",
    "clean": "rimraf lib .tmp",
    "coverage": "codecov -f ./coverage/lcov.info && codecov",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc -p . --noEmit",
    "next": "tsc -m commonjs && node lib/server.js",
    "preprod": "tsc -m commonjs && next build",
    "pretest": "npm run clean",
    "prod": "cross-env NODE_ENV=production node lib/server.js",
    "start": "concurrently -k 'yarn storybook' 'yarn next'",
    "storybook": "start-storybook -p 9001",
    "test": "jest"
  }
}
