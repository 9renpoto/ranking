{
  "name": "rate",
  "description": "TBD",
  "version": "0.0.1",
  "author": "9renpoto",
  "dependencies": {
    "@babel/core": "7.7.4",
    "@lingui/babel-preset-react": "2.8.3",
    "@lingui/react": "2.8.3",
    "@primer/components": "15.0.1",
    "@primer/octicons-react": "9.3.1",
    "@types/lingui__react": "2.8.1",
    "@types/styled-components": "4.4.0",
    "lerna": "3.19.0",
    "module-alias": "2.2.2",
    "next": "9.1.4",
    "npm-run-all": "4.1.5",
    "preact": "10.0.5",
    "preact-render-to-string": "5.1.1",
    "preact-ssr-prepass": "1.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rimraf": "3.0.0",
    "styled-components": "4.4.1",
    "typescript": "3.7.2",
    "typescript-styled-plugin": "0.15.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-react": "5.3.0",
    "@9renpoto/eslint-config-typescript": "5.3.0",
    "@9renpoto/stylelint-config": "5.3.0",
    "@lingui/cli": "2.8.3",
    "@lingui/macro": "2.8.3",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-storyshots": "5.2.6",
    "@storybook/addon-viewport": "5.2.6",
    "@storybook/react": "5.2.6",
    "@types/faker": "4.1.8",
    "@types/jest": "24.0.23",
    "@types/storybook__addon-storyshots": "5.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "2.7.1",
    "eslint": "6.7.1",
    "eslint-plugin-jest": "23.0.5",
    "faker": "4.1.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-styled-components": "6.3.4",
    "lerna-changelog": "0.8.3",
    "lint-staged": "9.5.0",
    "preact-render-to-json": "3.6.6",
    "prettier-eslint-cli": "5.0.0",
    "react-test-renderer": "16.12.0",
    "require-context.macro": "1.2.2",
    "stylelint": "12.0.0",
    "stylelint-config-prettier": "6.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "packages/**/src/**/*.{ts,tsx}",
      "apps/**/src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/.jest/setup.js"
    ],
    "setupFilesAfterEnv": [
      "jest-styled-components"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "lingui": {
    "sourceLocale": "en",
    "localeDir": "./apps/web/app/locale",
    "srcPathDirs": [
      "./apps",
      "./packages"
    ],
    "format": "po"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.{md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "private": true,
  "repository": "ssh://git@github.com/g59/rate.git",
  "scripts": {
    "build": "lerna run build --include-dependents",
    "bump": "lerna version --no-git-tag-version --no-push && lerna-changelog",
    "clean": "rimraf lib .tmp && lerna run clean && lerna clean --y",
    "lint": "run-p -l lint:*",
    "lint:es": "eslint . --ext=ts,tsx",
    "lint:prettier": "prettier --check '{packages,apps}/**/*.tsx'",
    "lint:stylelint": "stylelint '{packages,apps}/**/*.tsx'",
    "lint:tsc": "tsc -p . --noEmit",
    "test": "jest"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}
