{
  "name": "rate",
  "version": "0.0.1",
  "author": "9renpoto",
  "description": "TBD",
  "dependencies": {
    "@firebase/app-types": "0.3.7",
    "@fortawesome/fontawesome-free": "5.8.1",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@types/next": "8.0.4",
    "@types/styled-components": "4.1.14",
    "firebase": "5.10.1",
    "graphql-request": "1.8.2",
    "next": "8.1.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.7.0",
    "@9renpoto/eslint-config-react": "4.7.0",
    "@babel/core": "7.4.3",
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-knobs": "4.1.16",
    "@storybook/addon-storyshots": "4.1.16",
    "@storybook/addons": "4.1.18",
    "@storybook/cli": "4.1.16",
    "@storybook/react": "4.1.16",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.7",
    "@types/react-test-renderer": "16.8.1",
    "@types/storybook__addon-actions": "3.4.2",
    "@types/storybook__addon-knobs": "4.0.5",
    "@types/storybook__addon-storyshots": "3.4.9",
    "@types/storybook__react": "4.0.1",
    "@typescript-eslint/parser": "1.7.0",
    "@zeit/next-typescript": "1.1.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-require-context-hook": "1.0.0",
    "codecov": "3.3.0",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-jest": "22.5.1",
    "faker": "4.1.0",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "4.7.1",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5",
    "typescript-styled-plugin": "0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
    "collectCoverage": true,
    "setupFiles": [
      "./.storybook/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.{md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/g59/rate.git",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "tsc -m commonjs",
    "build:storybook": "build-storybook",
    "clean": "rimraf lib .tmp",
    "coverage": "codecov -f ./coverage/lcov.info && codecov",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc -p . --noEmit",
    "next": "tsc -m commonjs && node lib/server.js",
    "preprod": "tsc -m commonjs && next build",
    "pretest": "npm run clean",
    "prod": "cross-env NODE_ENV=production node lib/server.js",
    "start": "concurrently -k 'yarn storybook' 'yarn next'",
    "storybook": "start-storybook -p 9001",
    "test": "jest"
  }
}
