{
  "name": "rate",
  "version": "0.0.1",
  "author": "UNCOVER TRUTH Inc.",
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-svg-core": "1.2.8",
    "@fortawesome/free-solid-svg-icons": "5.5.0",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@types/next": "7.0.5",
    "firebase": "5.0.1",
    "graphql-request": "1.6.0",
    "next": "6.0.2",
    "next": "7.0.2",
    "next-routes": "9renpoto/next-routes#build",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "styled-components": "3.4.10"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.7.0",
    "@9renpoto/eslint-config-react": "4.7.0",
    "@9renpoto/tslint-config": "4.7.0",
    "@babel/core": "7.1.6",
    "@storybook/addon-actions": "4.0.8",
    "@storybook/addon-knobs": "4.0.8",
    "@storybook/addon-storyshots": "4.0.8",
    "@storybook/addons": "4.0.8",
    "@storybook/cli": "4.0.8",
    "@storybook/react": "4.0.8",
    "@types/faker": "4.1.4",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.10",
    "@types/react-test-renderer": "16.0.3",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-knobs": "3.4.1",
    "@types/storybook__react": "4.0.0",
    "@zeit/next-typescript": "1.1.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-require-context-hook": "1.0.0",
    "codecov": "3.1.0",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "eslint": "5.9.0",
    "eslint-plugin-jest": "22.0.0",
    "faker": "4.1.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "react-test-renderer": "16.6.3",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.1",
    "typescript-styled-plugin": "0.13.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "./.storybook/setup.js"
    ],
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.storybook.json"
      }
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/uncovertruth/rate.git",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "tsc -m commonjs",
    "build:storybook": "build-storybook",
    "clean": "rimraf lib .tmp",
    "coverage": "codecov -f ./coverage/lcov.info && codecov",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc -p tsconfig.storybook.json",
    "lint:tslint": "tslint '{app,components,pages}/**/*.ts{,x}'",
    "next": "tsc -m commonjs && node lib/server.js",
    "preprod": "tsc -m commonjs && next build",
    "pretest": "npm run clean",
    "prod": "cross-env NODE_ENV=production node lib/server.js",
    "start": "concurrently -k 'yarn storybook' 'yarn next'",
    "storybook": "start-storybook -p 9001",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
