{
  "name": "rate",
  "version": "0.0.1",
  "author": "UNCOVER TRUTH Inc.",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-11",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-8",
    "@fortawesome/react-fontawesome": "^0.1.0-8",
    "@types/next": "^2.4.9",
    "next": "^5.1.0",
    "next-routes": "9renpoto/next-routes#build",
    "prop-types": "^15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.2",
    "@storybook/addon-knobs": "3.4.2",
    "@storybook/addon-storyshots": "3.4.2",
    "@storybook/addons": "3.4.2",
    "@storybook/cli": "3.4.2",
    "@storybook/react": "3.4.2",
    "@types/faker": "4.1.2",
    "@types/jest": "22.2.3",
    "@types/node": "^9.6.5",
    "@types/react-test-renderer": "16.0.1",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-knobs": "3.3.1",
    "@types/storybook__react": "3.0.7",
    "@uncovertruth/eslint-config": "4.2.0",
    "@uncovertruth/eslint-config-react": "4.3.1",
    "@uncovertruth/tslint-config": "4.3.1",
    "@zeit/next-typescript": "0.1.1",
    "awesome-typescript-loader": "4.0.1",
    "babel-core": "6.26.0",
    "babel-runtime": "6.26.0",
    "codecov": "3.0.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "eslint": "4.19.1",
    "eslint-plugin-jest": "21.15.0",
    "faker": "4.1.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.4",
    "npm-run-all": "4.1.2",
    "prettier-eslint-cli": "4.7.1",
    "react-test-renderer": "16.3.2",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.4",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.8.3",
    "typescript-eslint-parser": "15.0.0",
    "typescript-styled-plugin": "0.6.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.storybook.json"
      }
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/uncovertruth/rate.git",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "tsc -m commonjs",
    "build:storybook": "build-storybook",
    "clean": "rimraf lib .tmp",
    "coverage": "codecov -f ./coverage/lcov.info && codecov",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc -p tsconfig.storybook.json",
    "lint:tslint": "tslint '{app,components,pages}/**/*.ts{,x}'",
    "next": "tsc -m commonjs && node lib/server.js",
    "precommit": "lint-staged",
    "preprod": "tsc -m commonjs && next build",
    "pretest": "npm run clean",
    "prod": "cross-env NODE_ENV=production node lib/server.js",
    "start": "concurrently -k 'yarn storybook' 'yarn next'",
    "storybook": "start-storybook -p 9001",
    "test": "jest"
  }
}
