{
  "name": "rate",
  "description": "TBD",
  "version": "0.0.1",
  "author": "9renpoto",
  "_moduleAliases": {
    "react": "node_modules/preact/compat",
    "react-dom": "node_modules/preact/compat",
    "react-ssr-prepass": "node_modules/preact-ssr-prepass"
  },
  "dependencies": {
    "@babel/core": "7.6.4",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "@types/styled-components": "4.1.19",
    "lerna": "3.18.2",
    "module-alias": "2.2.2",
    "next": "9.1.1",
    "npm-run-all": "4.1.5",
    "preact": "10.0.1",
    "preact-render-to-string": "5.1.0",
    "preact-ssr-prepass": "1.0.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "rimraf": "3.0.0",
    "styled-components": "4.4.0",
    "typescript": "3.6.4",
    "typescript-styled-plugin": "0.15.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-react": "5.2.1",
    "@9renpoto/eslint-config-typescript": "5.2.1",
    "@9renpoto/stylelint-config": "5.2.1",
    "@storybook/addon-actions": "5.2.4",
    "@storybook/addon-storyshots": "5.2.4",
    "@storybook/addon-viewport": "5.2.4",
    "@storybook/preact": "5.2.4",
    "@types/faker": "4.1.6",
    "@types/jest": "24.0.19",
    "@types/storybook__addon-storyshots": "5.1.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "2.6.1",
    "eslint": "6.5.1",
    "eslint-plugin-jest": "22.19.0",
    "faker": "4.1.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "jest-styled-components": "6.3.3",
    "lerna-changelog": "0.8.2",
    "lint-staged": "9.4.2",
    "preact-render-to-json": "3.6.6",
    "prettier-eslint-cli": "5.0.0",
    "prettier-stylelint": "0.4.2",
    "react-test-renderer": "16.10.2",
    "require-context.macro": "1.2.2",
    "stylelint": "11.1.1",
    "stylelint-config-prettier": "6.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "packages/**/src/**/*.{ts,tsx}",
      "apps/**/src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/.jest/setup.js"
    ],
    "setupFilesAfterEnv": [
      "jest-styled-components"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "prettier-stylelint --write",
      "git add"
    ],
    "*.{md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/g59/rate.git",
  "scripts": {
    "build": "lerna run build --include-dependents",
    "bump": "lerna version --no-git-tag-version --no-push && lerna-changelog",
    "clean": "rimraf lib .tmp && lerna run clean && lerna clean --y",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --ext=ts,tsx",
    "lint:prettier": "prettier --check '{packages,apps}/**/*.tsx'",
    "lint:stylelint": "stylelint '{packages,apps}/**/*.tsx'",
    "lint:tsc": "tsc -p . --noEmit",
    "start": "start-storybook -p 9001",
    "test": "jest"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}
