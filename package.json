{
  "name": "rate",
  "version": "0.0.1",
  "author": "UNCOVER TRUTH Inc.",
  "dependencies": {
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "styled-components": "3.2.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.1",
    "@storybook/addon-knobs": "3.4.1",
    "@storybook/addon-storyshots": "3.4.1",
    "@storybook/addons": "3.4.1",
    "@storybook/cli": "3.4.1",
    "@storybook/react": "3.4.1",
    "@types/jest": "22.2.2",
    "@types/react": "16.3.8",
    "@types/react-dom": "16.0.5",
    "@types/react-test-renderer": "16.0.1",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-knobs": "3.3.0",
    "@types/storybook__react": "3.0.7",
    "@uncovertruth/tslint-config": "4.3.0",
    "awesome-typescript-loader": "4.0.0",
    "babel-core": "6.26.0",
    "babel-runtime": "6.26.0",
    "codecov": "3.0.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.4",
    "prettier": "1.11.1",
    "react-test-renderer": "16.3.1",
    "ts-jest": "22.4.2",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.8.1",
    "typescript-styled-plugin": "0.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.{md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/uncovertruth/rate.git",
  "scripts": {
    "build": "build-storybook",
    "coverage": "codecov -f ./coverage/lcov.info && codecov",
    "lint": "tslint 'components/**/*.ts'",
    "test": "jest",
    "precommit": "lint-staged",
    "start": "start-storybook -p 9001"
  }
}
