{
  "name": "rate",
  "description": "TBD",
  "version": "0.0.1",
  "author": "9renpoto",
  "dependencies": {
    "@firebase/app-types": "0.4.3",
    "@fortawesome/fontawesome-free": "5.11.1",
    "@fortawesome/fontawesome-svg-core": "1.2.24",
    "@fortawesome/free-solid-svg-icons": "5.11.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@types/styled-components": "4.1.19",
    "firebase": "6.6.2",
    "next": "9.0.3",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "5.1.0",
    "@9renpoto/eslint-config-react": "5.1.0",
    "@9renpoto/stylelint-config": "5.1.0",
    "@babel/core": "7.4.5",
    "@storybook/addon-actions": "5.1.11",
    "@storybook/addon-storyshots": "5.1.11",
    "@storybook/addon-viewport": "5.1.11",
    "@storybook/react": "5.1.11",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.18",
    "@types/react-test-renderer": "16.9.0",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__addon-storyshots": "5.1.1",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/parser": "2.3.0",
    "@zeit/next-typescript": "1.1.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "codecov": "3.6.1",
    "concurrently": "4.1.2",
    "cross-env": "6.0.0",
    "eslint": "6.4.0",
    "eslint-plugin-jest": "22.17.0",
    "faker": "4.1.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "jest-styled-components": "6.3.3",
    "lerna": "^3.16.4",
    "lerna-changelog": "0.8.2",
    "lint-staged": "9.3.0",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "prettier-stylelint": "0.4.2",
    "react-test-renderer": "16.9.0",
    "rimraf": "3.0.0",
    "stylelint": "11.0.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.1.2",
    "typescript": "3.6.3",
    "typescript-styled-plugin": "0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "jest-styled-components"
    ],
    "setupFiles": [
      "./.storybook/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "prettier-stylelint --write",
      "git add"
    ],
    "*.{md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": "ssh://git@github.com/g59/rate.git",
  "scripts": {
    "build": "lerna run build --include-filtered-dependents",
    "bump": "lerna version --no-git-tag-version --no-push && lerna-changelog",
    "clean": "rimraf lib .tmp && lerna run clean && lerna clean --y",
    "coverage": "codecov -f ./coverage/lcov.info && codecov",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc -p . --noEmit",
    "next": "tsc -m commonjs && node lib/server.js",
    "preprod": "tsc -m commonjs && next build",
    "pretest": "npm run clean",
    "prod": "cross-env NODE_ENV=production node lib/server.js",
    "start": "concurrently -k 'yarn storybook' 'yarn next'",
    "storybook": "start-storybook -p 9001",
    "test": "jest"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}
